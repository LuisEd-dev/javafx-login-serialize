<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane maxHeight="350.0" maxWidth="500.0" minHeight="350.0" minWidth="500.0" prefHeight="350.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="HOME">
         <font>
            <Font name="System Bold" size="24.0" />
         </font></Label>
      <Label layoutX="14.0" layoutY="107.0" text="Usuário:">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Button layoutX="14.0" layoutY="165.0" mnemonicParsing="false" onMouseClicked="#deleteUsuario" prefHeight="25.0" prefWidth="190.0" text="Deletar" textFill="#a11f1f">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <Button layoutX="14.0" layoutY="205.0" mnemonicParsing="false" onMouseClicked="#editeUsuario" prefHeight="25.0" prefWidth="190.0" text="Editar" textFill="#2030a1">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <Label layoutX="14.0" layoutY="49.0" text="Lista de Usuários:">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <ComboBox fx:id="combo" layoutX="14.0" layoutY="130.0" onMouseClicked="#carregarUsuarios" prefHeight="25.0" prefWidth="190.0" promptText="Escolha o Usuário" />
   </children>
</Pane>
